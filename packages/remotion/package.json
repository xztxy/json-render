{
  "name": "@json-render/remotion",
  "version": "0.11.0",
  "license": "Apache-2.0",
  "description": "Remotion renderer for @json-render/core. JSON becomes video compositions.",
  "keywords": [
    "json",
    "video",
    "remotion",
    "ai",
    "generative-video",
    "llm",
    "renderer",
    "streaming",
    "compositions"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vercel-labs/json-render.git",
    "directory": "packages/remotion"
  },
  "homepage": "https://github.com/vercel-labs/json-render#readme",
  "bugs": {
    "url": "https://github.com/vercel-labs/json-render/issues"
  },
  "publishConfig": {
    "access": "public"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./server": {
      "types": "./dist/server.d.ts",
      "import": "./dist/server.mjs",
      "require": "./dist/server.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "check-types": "tsc --noEmit",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@json-render/core": "workspace:*"
  },
  "devDependencies": {
    "@internal/typescript-config": "workspace:*",
    "@types/react": "19.2.3",
    "remotion": "4.0.418",
    "tsup": "^8.0.2",
    "typescript": "^5.4.5",
    "zod": "^4.3.6"
  },
  "peerDependencies": {
    "react": "^19.2.3",
    "remotion": "^4.0.0",
    "zod": "^4.0.0"
  }
}
