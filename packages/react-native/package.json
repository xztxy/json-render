{
  "name": "@json-render/react-native",
  "version": "0.4.3",
  "license": "Apache-2.0",
  "description": "React Native renderer for @json-render/core. JSON becomes React Native components.",
  "keywords": [
    "json",
    "ui",
    "react-native",
    "ai",
    "generative-ui",
    "llm",
    "renderer",
    "streaming",
    "components"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vercel-labs/json-render.git",
    "directory": "packages/react-native"
  },
  "homepage": "https://github.com/vercel-labs/json-render#readme",
  "bugs": {
    "url": "https://github.com/vercel-labs/json-render/issues"
  },
  "publishConfig": {
    "access": "public"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./schema": {
      "types": "./dist/schema.d.ts",
      "import": "./dist/schema.mjs",
      "require": "./dist/schema.js"
    },
    "./catalog": {
      "types": "./dist/catalog.d.ts",
      "import": "./dist/catalog.mjs",
      "require": "./dist/catalog.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "check-types": "tsc --noEmit",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@json-render/core": "workspace:*"
  },
  "devDependencies": {
    "@repo/typescript-config": "workspace:*",
    "@types/react": "19.2.3",
    "react-native": "0.83.1",
    "tsup": "^8.0.2",
    "typescript": "^5.4.5",
    "zod": "^4.0.0"
  },
  "peerDependencies": {
    "react": "^19.0.0",
    "react-native": ">=0.71.0",
    "zod": "^4.0.0"
  }
}
